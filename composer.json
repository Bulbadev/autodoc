{
    "name": "bulbadev/autodoc",
    "description": "Generator api documentation using laravel formRequests and tests",
    "version": "1.0.0",
    "type": "library",
    "keywords": [
        "laravel",
        "swagger",
        "testing",
        "auto-documentation",
        "rest-api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eugene Kozlov",
            "email": "kozlov.eugene.88@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "minime/annotations": "dev-master",
        "illuminate/console": ">=7.0",
        "illuminate/support": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.0",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "Bulbadev\\Autodoc\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bulbadev\\Autodoc\\AutodocServiceProvider"
            ]
        }
    },

    "minimum-stability": "dev"
}
